{
  "extends": "lighthouse:default",
  "settings": {
    "formFactor": "desktop",
    "screenEmulation": {
      "mobile": false,
      "width": 1350,
      "height": 940,
      "deviceScaleRatio": 1,
      "disabled": false
    },
    "throttlingMethod": "simulate",
    "throttling": {
      "rttMs": 40,
      "throughputKbps": 10240,
      "cpuSlowdownMultiplier": 1,
      "requestLatencyMs": 0,
      "downloadThroughputKbps": 10240,
      "uploadThroughputKbps": 10240
    }
  }
}